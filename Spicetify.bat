for /f "delims=: tokens=*" %%A in ('findstr /b ::: "%~f0"') do @echo(%%A

timeout /t 3 >nul

echo ___________________________________________________________________________________________________________
@ping -n 1 -w 1000 0.0.0.1 > NUL  
echo ^| 1 ^| Install Spicetify    ^| Installs Spicetify                                                           ^|
@ping -n 1 -w 1000 0.0.0.1 > NUL  
echo ^|_________________________________________________________________________________________________________^|
@ping -n 1 -w 1000 0.0.0.1 > NUL  
echo ^| 2 ^| Uninstall Spicetify  ^| Uninstalls Spicetify                                                         ^|
@ping -n 1 -w 1000 0.0.0.1 > NUL  
echo ^|_________________________________________________________________________________________________________^|
@ping -n 1 -w 1000 0.0.0.1 > NUL  
echo ^| 3 ^| Install Spotify      ^| Installs the correct Spotify                                                 ^|
@ping -n 1 -w 1000 0.0.0.1 > NUL  
echo ^|_________________________________________________________________________________________________________^|
@ping -n 1 -w 1000 0.0.0.1 > NUL  
